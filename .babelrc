    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        [
            "inline-import-data-uri",
            {
                "extensions": [
                    "png",
                    "jpg",
                    "svg"
                ]
            }
        ]
    ]
}